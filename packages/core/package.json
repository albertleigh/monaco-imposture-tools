    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.14.31",
    "@types/sinon": "^10.0.2",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
