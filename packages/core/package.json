  "main": "./lib/main.js",
  "typings": "./lib/typings/main.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "compile:tsc": "tsc",
    "compile:tsc-prod": "tsc --project tsconfig.prod.json",
    "compile": "npm run clean && npm run compile:tsc-prod",
    "compile:debug": "npm run clean && npm run compile:tsc",
    "test": "node node_modules/mocha/bin/mocha -s -0 --file \"./test/setup.js\" \"./test/**/*.spec.js\"",
    "test:coverage": "nyc --reporter=text mocha -s -0 --file \"./test/setup.js\" \"./test/**/*.spec.js\"",
    "coverage": "nyc --no-clean --temp-dir=../../.nyc_output mocha -s -0 --file \"./test/setup.js\" \"./test/**/*.spec.js\"",
    "normalize-grammar": "ts-node scripts/normalize-grammar.ts",
    "print-rules": "ts-node scripts/print-rules.ts",
    "postpublish": "node ./scripts/post-publish.js",
    "lint": "eslint --ext=js,jsx,ts,tsx src",
    "lint:fix": "eslint --ext=js,jsx,ts,tsx src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:albertleigh/monaco-imposture-tools.git"
  },
  "dependencies": {
    "@monaco-imposture-tools/oniguruma-asm": "^0.1.3",
    "fast-plist": "^0.1.2"
  },
  "devDependencies": {
